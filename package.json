{
  "name": "sistema-seguridad-facial",
  "version": "1.0.0",
  "description": "Sistema de Seguridad con Reconocimiento Facial - Tesina",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "setup": "npm install && npx prisma generate && npx prisma db push && npx prisma db seed",
    "start:services": "concurrently \"npm run start:python\" \"npm run dev\"",
    "start:python": "cd face_recognition_service && python main.py",
    "generate-keys": "node -e \"const crypto = require('crypto'); console.log('JWT_SECRET=' + crypto.randomBytes(64).toString('hex')); console.log('NEXTAUTH_SECRET=' + crypto.randomBytes(64).toString('hex')); console.log('API_KEY=' + crypto.randomBytes(32).toString('hex'));\""
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
